        _expressui.grid.getSelectionsHasMessage = function (grid) {
            var selections = [];
            if (grid.type) {
                switch (grid.type) {
                    case 'datagrid':
                        selections = $(grid.selector).datagrid('getSelections');
                        if (!selections) {
                            _expressui.messager.alert({msg: '请先勾选要操作的行'});
                            return;
                        } else {
                            if (selections.length <= 0) {
                                _expressui.messager.alert({msg: '请先勾选要操作的行'});
                                return;
                            }
                        }
                        break;

                    case 'treegrid':
                        selections = $(grid.selector).treegrid('getSelections');
                        if (!selections) {
                            _expressui.messager.alert({msg: '请先勾选要操作的行'});
                            return;
                        } else {
